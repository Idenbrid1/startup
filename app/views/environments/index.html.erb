 <%= render partial: "/layouts/navbar_coperatesite" %>
 <!-- JavaScript -->
   <link rel="stylesheet" href="https://unpkg.com/flickity@2/dist/flickity.min.css">

 <!-- JavaScript -->
   <script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
   <script type="text/javascript">
    var jq= $.noConflict(true);
    jq('.carousel').flickity({
      // options
      cellAlign: 'left',
      contain: true,
      autoplay: true,
      interval: 1500
    });
   </script>
   <style type="text/css">
                

.carousel {
  background: #fff;
}
.flickity-prev-next-button{
  display: none;
}
.carousel-cell {
  width: 60%;
  height: 450px;
  margin-right: 30px;
  background: #fff;
  border-radius: 5px;
  counter-increment: carousel-cell;
}
.carousel-cell img{
  width: 100%;
}
body, p, ol, ul, td{
  font-family: 'Libre Franklin';
}
/* cell number */
.carousel-cell:before {
  display: block;
  text-align: center;
  /*content: counter(carousel-cell);*/
  line-height: 400px;
  font-size: 80px;
  color: white;
}
.flickity-page-dots .dot {
    display: inline-block;
    width: 15px;
    height: 15px;
    margin: 0px 8px;
    background: #808080 !important;
    opacity: 0.25;
    border-radius: 50%;
    cursor: pointer;
}

@media(max-width: 600px){
  .carousel-cell {
  width: 60%;
  height: 200px;
  counter-increment: carousel-cell;
}
.mtbm-0{
  margin-top: 0 !important;
  margin-bottom: 0!important;
  padding-top: 0 !important;
}
}
</style>
    
   <style type="text/css">
   *:focus{
              outline: none;
            }

            body{
              overflow-x: hidden;
              margin: 0;
              font-family: 'Libre Franklin', sans-serif;
              padding: 0;
              box-sizing: border-box;
            }
            p{
              font-size: 100%;
              line-height: 2;
              letter-spacing: 1.2px;
              color: #9DA0A4 !important;
            }
            strong  {
              color:#000 !important;
            }
            ul li {
              font-size: 110%;
              line-height: 1.45;
              letter-spacing: 1.2px;
              font-weight: 500;
              margin-bottom: 1rem;
              z-index: 5;
            }
            .sns-buttons li{
              margin-bottom: 0 !important;
            }
            .listUl a {
             color: #9DA0A4 !important;
              text-decoration: none;
              background-color: transparent;
          }
            .listUl  a:hover {
             color: #9DA0A4;
              text-decoration: none;
              background-color: transparent;
          }      

  </style>
